setTimeout(function() {
  $('#recent_posts')
    .html('<%= j render @user.recent_posts, is_blurb: true %>');
  $('#recent_shared_posts')
    .html('<%= j render @user.recent_shared_posts, is_blurb: true %>');
  $('#recent_comments')
    .html('<%= j render "users/recent_comments", comments: @user.recent_comments %>');


  $('#user_loader').spin(false);
}, 300);